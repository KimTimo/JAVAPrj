
public class Avatar {

	public static void main(String[] args) {
		
		double valueOfSupply = 10000.0;
		double vatRate = 0.1;
		double expenseRate = 0.3;
		double vat = valueOfSupply*vatRate;
		System.out.println("VAT : "+vat);
		System.out.println("Value Of Supply : "+ valueOfSupply);
		double total = valueOfSupply+vat;
		System.out.println("Total : "+total);
		double expense = valueOfSupply*expenseRate;
		System.out.println("Expense : "+expense);
		System.out.println("Income : "+(valueOfSupply-expense));
		System.out.println("Dividend1 : "+(valueOfSupply-expense)*0.5);
		System.out.println("Dividend2 : "+(valueOfSupply-expense)*0.3);
		System.out.println("Dividend3 : "+(valueOfSupply-expense)*0.2);
		
		


	}

}
